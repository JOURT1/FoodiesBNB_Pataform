{
  "version": 2,
  "name": "foodiesbnb",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/foodiesbnb"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=31536000,immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.js)",
      "headers": [
        {
          "key": "cache-control", 
          "value": "s-maxage=31536000,immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=31536000,immutable"
        }
      ]
    }
  ]
}
